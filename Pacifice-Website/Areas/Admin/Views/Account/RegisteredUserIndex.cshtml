
@{
    ViewData["Title"] = "RegisteredUserIndex";
}


<div class="row">
    <div class="col-md-12">
        <div class="card mb-4">
            <div class="card-header bg-info">
                <span class=" text-white font-weight-normal"><b>All User </b></span>
            </div>
            <div class="table-responsive">
                @if (Model != null)
                {

                <table class="table table-bordered table-responsive table-hover">
                    <thead>
                        <tr>
                            <td>Email</td>
                            <td>Pasword</td>
                        </tr>

                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.Email</td>
                                <td>@item.PasswordHash</td>

                            </tr>

                        }

                    </tbody>
                </table>
                }
            </div>
        </div>
    </div>
</div>

<br>
<br>